{
  "name": "@airbyte-embedded/airbyte-embedded-widget",
  "version": "0.4.4",
  "description": "Embedded widget for Airbyte",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/airbyte-embedded-widget.iife.js",
  "jsdelivr": "dist/airbyte-embedded-widget.iife.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/airbyte-embedded-widget.iife.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "clean": "rm -rf dist",
    "build": "rollup -c && tsc --declaration --emitDeclarationOnly --declarationDir dist",
    "prepare": "pnpm run clean && pnpm run build"
  },
  "keywords": [
    "airbyte",
    "embedded",
    "modal",
    "widget"
  ],
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.11.24",
    "eslint": "^9.23.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.1.0",
    "rollup": "^4.0.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.1.2",
    "typescript": "5.8.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "overrides": {
      "typescript": "5.8.2",
      "@types/node": "20.17.32",
      "eslint": "9.25.1"
    }
  },
  "packageManager": "pnpm@9.15.9"
}
